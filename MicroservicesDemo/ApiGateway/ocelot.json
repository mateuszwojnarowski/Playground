{
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost"
  },
  "Routes": [
    {
      "DownstreamPathTemplate": "/orders/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7117
        }
      ],
      "UpstreamPathTemplate": "/api/orders/{everything}",
      "SwaggerKey": "orders"
    },
    {
      "DownstreamPathTemplate": "/products/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7228
        }
      ],
      "UpstreamPathTemplate": "/api/products/{everything}",
      "SwaggerKey": "products"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "products",
      "Config": [
        {
          "Name": "Products",
          "Version": "v1",
          "Url": "https://localhost:7228/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "orders",
      "Config": [
        {
          "Name": "Orders",
          "Version": "v1",
          "Url": "https://localhost:7117/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}